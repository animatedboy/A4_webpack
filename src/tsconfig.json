{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    //"suppressImplicitAnyIndexErrors": true,
    "outDir": "../build",
    "noImplicitAny": false,
    "declaration": true,
    "removeComments": false,
    "listEmittedFiles":true
  },
  "exclude": [
    "node_modules"
  ]

}
